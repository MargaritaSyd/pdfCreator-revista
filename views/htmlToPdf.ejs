<body>
    <header>

    </header>
    
    <main class="mainPdf">
    <div style="display: flex; flex-direction: column; position: relative; left: 640;"><b>P</b><b>E</b><b>D</b><b>I</b><b>G</b><b>R</b><b>E</b><b>E</b></div>
    <div style="display:flex; position: relative; top: -160; border: 2px solid black; margin: 15px; padding: 4px; height: 250mm;">
    <form id="form"    action="/pdf/<%=ejemplarX.id%>" method="POST" >
        <div style="display: flex; flex-wrap: nowrap; flex-direction: row;">
            <div class="contentDescriptStud" style="display: flex; flex-wrap: nowrap; flex-direction: column; width: 40%;">
                <b class="ejemplarNombre"> <%=ejemplarX.nombre%> </b>
                <% if(ejemplarX.sexo == "H"){ %>
                <%    ejemplarX.sexo = "Hembra"%>
                <%} else {%>
                <%      ejemplarX.sexo = "Macho" %>

                <%}%>
                <% if(ejemplarX.pelo == "ZC"){ %>
                    <% ejemplarX.pelo = "Zaino Colorado" %>
                <%} else if(ejemplarX.pelo == "A") {%>    
                    <% ejemplarX.pelo = "Alazan" %>
                <%} else if(ejemplarX.pelo == "AT") {%>    
                    <% ejemplarX.pelo = "Alazan Tostado" %>
                <%} else if(ejemplarX.pelo == "O") {%>    
                    <% ejemplarX.pelo = "Oscuro" %>
                <%} else if(ejemplarX.pelo == "M") {%>    
                    <% ejemplarX.pelo = "Moro" %>
                <%} else if(ejemplarX.pelo == "R") {%>    
                    <% ejemplarX.pelo = "Rosillo" %>
                <%} else if(ejemplarX.pelo == "T") {%>    
                    <% ejemplarX.pelo = "Tordillo" %>
                <%} else if(ejemplarX.pelo == "Z") {%>    
                    <% ejemplarX.pelo = "Zaino" %>
                <%} else if(ejemplarX.pelo == "ZD") {%>    
                    <% ejemplarX.pelo = "Zaino Doradillo" %>
                <%} else {%>    
                    <% ejemplarX.pelo = "Zaino Negro" %> 
                <%}%>

                <% if(ejemplarX.mes_nac == 1){ %>
                    <% ejemplarX.mes_nac = "Enero" %>
                <%} else if(ejemplarX.mes_nac == 2) {%>    
                    <% ejemplarX.mes_nac = "Febrero" %>
                <%} else if(ejemplarX.mes_nac == 3) {%>    
                    <% ejemplarX.mes_nac = "Marzo" %>
                <%} else if(ejemplarX.mes_nac == 4) {%>    
                    <% ejemplarX.mes_nac = "Abril" %>
                <%} else if(ejemplarX.mes_nac == 5) {%>    
                    <% ejemplarX.mes_nac = "Mayo" %>
                <%} else if(ejemplarX.mes_nac == 6) {%>    
                    <% ejemplarX.mes_nac = "Junio" %>
                <%} else if(ejemplarX.mes_nac == 7) {%>    
                    <% ejemplarX.mes_nac = "Julio" %>
                <%} else if(ejemplarX.mes_nac == 8) {%>    
                    <% ejemplarX.mes_nac = "Agosto" %>
                <%} else if(ejemplarX.mes_nac == 9) {%>    
                    <% ejemplarX.mes_nac = "Septiembre" %>
                <%} else if(ejemplarX.mes_nac == 10) {%>    
                    <% ejemplarX.mes_nac = "Octubre" %>
                <%} else if(ejemplarX.mes_nac == 11) {%>    
                    <% ejemplarX.mes_nac = "Noviembre" %>  
                <%} else {%>    
                    <% ejemplarX.mes_nac = "Diciembre" %> 
                <%}%>

                <b class="descriptEjemplar"> <%=ejemplarX.sexo%>, <%=ejemplarX.pelo%>, nacido el <%=ejemplarX.dia_nac%> de <%=ejemplarX.mes_nac%> de <%=ejemplarX.anio_nac%> - <%=ejemplarX.familia%></b>
                <input type="text" style="display: none;" name="ejemplarSexo" value=<%=ejemplarX.sexo%>>
                <input type="text" style="display: none;" name="ejemplarPelo" value=<%=ejemplarX.pelo%>>
                <input type="text" style="display: none;" name="ejemplarDia_nac" value=<%=ejemplarX.dia_nac%>>
                <input type="text" style="display: none;" name="ejemplarMes_nac" value=<%=ejemplarX.mes_nac%>>  
                <input type="text" style="display: none;" name="ejemplarAnio_nac" value=<%=ejemplarX.anio_nac%>>
                <input type="text" style="display: none;" name="ejemplarFamilia" value=<%=ejemplarX.familia%>>
                
            </div>
            <div class="borderContentCarreras">
               
                <div class="detalleCarrerasCorridas">
                    <div class="cadaDetalleCarreras" style="display: flex; flex-direction: column; width: 12%;">
                        <b> Año </b>
                        <b> <%= anios[0].slice(2)%>-<%=anios[anios.length-1].slice(2)%></b>
                        <input type="text" style="display: none;" name="aniosCorrio" value=<%= anios[0].slice(2)%>-<%=anios[anios.length-1].slice(2)%>>
                    </div>
                    <div class="cadaDetalleCarreras" style="display: flex; flex-direction: column; width: 12%;">
                        <b> Edad</b>
                        <b><%= anios[0] - ejemplarX.anio_nac%> - <%=anios[anios.length-1]- ejemplarX.anio_nac%></b>
                        <input type="text" style="display: none;" name="edadCorrio" value=<%= anios[0] - ejemplarX.anio_nac%> - <%=anios[anios.length-1]- ejemplarX.anio_nac%>>
                    </div>
                    <div class="cadaDetalleCarreras" style="display: flex; flex-direction: column; width: 12%;">
                        <b>Corrio</b>
                        <b> <%= totalLargadas%> </b><input type="text" style="display: none;" name="totalLargadas" value=<%=totalLargadas%>>
                    </div>
                    <div class="cadaDetalleCarreras" style="display: flex; flex-direction: column; width: 12%;">
                        <b>1º</b>
                        <b><%=totalGanadas%></b><input type="text" style="display: none;" name="totalGanadas" value=<%=totalGanadas%>>
                    </div>
                    <div class="cadaDetalleCarreras" style="display: flex; flex-direction: column; width: 12%;">
                        <b>2º</b>
                        <b><%=totalSegundos%></b><input type="text" style="display: none;" name="totalSegundos" value=<%=totalSegundos%>>
                    </div>
                    <div class="cadaDetalleCarreras" style="display: flex; flex-direction: column; width: 12%;">
                        <b>3º</b>
                        <b><%=totalTerceros%></b><input type="text" style="display: none;" name="totalTerceros" value=<%=totalTerceros%>>
                    </div>
                    <div class="premioDetalleCarreras" style="display: flex; flex-direction: column; width: 25%;">
                        <b>Premios</b>
                        <b>$ <%=totalGanancias%></b><input type="text" style="display: none;" name="ganancias" value=<%=totalGanancias%>>
                    </div>

                </div>
               
            </div>
        </div>
